{
  "name": "backend",
  "version": "1.0.0",
  "description": "API para el sistema de gestión de equipos con préstamos y mantenimiento",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch src --ext ts,js,json --exec tsx src/server.ts",
    "seed:admin": "tsx src/seed/create-admin.ts",
